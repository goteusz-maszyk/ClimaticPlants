{
  "schemaVersion": 1,
  "id": "climatic_plants",
  "version": "${version}",
  "name": "ClimaticPlants",
  "description": "Temperature now matters, no more growing spruces in a tropical forest!",
  "authors": ["gotitim"],
  "contact": {
    "homepage": "https://modrinth.com/mod/climatic-plants",
    "sources": "https://github.com/goteusz-maszyk/ClimaticPlants",
    "issues": "https://github.com/goteusz-maszyk/ClimaticPlants/issues"
  },
  "license": "AGPL-3.0",
  "icon": "assets/climatic_plants/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "dev.gotitim.climatic_plants.client.ClimaticPlantsClient"
    ],
    "main": [
      "dev.gotitim.climatic_plants.ClimaticPlants"
    ],
    "fabric-datagen": [
      "dev.gotitim.climatic_plants.datagen.ClimaticPlantsDataGenerator"
    ]
  },
  "mixins": [
    "climatic_plants.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": ">=${fabric_version}",
    "minecraft": "${minecraft_version}",
